class SpyNumber
{
	public static void main(String[] args) 
	{
		int num = 123;
		int sum = 0;
		int prod = 1;

		while (num > 0)
		{
			int rem = num % 10;
			sum = sum + num;
			prod = prod*num;
			num /= 10;
		}
		if (sum == prod)
		{
			System.out.println("Spy Number");
		}
		else{
			System.out.println("Not a Spy Number");
		}
	}
}
